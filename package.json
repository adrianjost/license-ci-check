{
	"name": "license-ci-checker",
	"version": "1.0.3",
	"description": "Checks the compatiblity of your license with your dependencies licenses",
	"main": "index.js",
	"scripts": {
		"lcc": "node index.js",
		"lint:prettier": "prettier --write",
		"lint:all:prettier": "yarn lint:prettier -c \"**/*.{js,json,md}\"",
		"lint": "run-s lint:all:*"
	},
	"keywords": [
		"license",
		"compatibility",
		"check",
		"npm-license-crawler"
	],
	"author": "Adrian Jost",
	"license": "MIT",
	"dependencies": {
		"colors": "^1.3.3",
		"npm-license-crawler": "^0.2.0"
	},
	"repository": {
		"type": "git",
		"url": "http://github.com/adrianjost/license-ci-check.git"
	},
	"bin": {
		"license-ci-check": "./index.js",
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4"
	}
}
